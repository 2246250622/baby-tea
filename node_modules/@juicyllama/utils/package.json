  },
  "devDependencies": {
    "@juicyllama/dev": "^0.2.0"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "start:dev": "tsc -w",
    "format": "prettier --write \"src/**/*.ts\"",
