{
  "name": "unplugin-swc",
  "version": "1.4.4",
  "packageManager": "pnpm@8.11.0",
  "description": "SWC plugin for Vite and Rollup",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "author": "EGOIST <hi@egoist.dev> (https://github.com/egoist)",
  "contributors": [
    "hannoeru <me@hanlee.co> (https://github.com/hannoeru)"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/unplugin/unplugin-swc.git"
  },
  "bugs": {
    "url": "https://github.com/unplugin/unplugin-swc/issues"
  },
  "homepage": "https://github.com/unplugin/unplugin-swc/tree/main/#readme",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "npm run build && vitest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "npm run build",
    "type-check": "tsc --noEmit"
  },
  "peerDependencies": {
    "@swc/core": "^1.2.108"
  },
  "dependencies": {
    "@rollup/pluginutils": "^5.1.0",
    "load-tsconfig": "^0.2.5",
    "unplugin": "^1.5.1"
  },
  "devDependencies": {
    "@hannoeru/eslint-config": "^0.9.7",
    "@swc/core": "^1.3.100",
    "@types/node": "^18.19.2",
    "defu": "^6.1.3",
    "esbuild": "0.19.8",
    "path-exists": "^5.0.0",
    "prettier": "3.1.0",
    "rollup": "^3.29.4",
    "tsup": "7.2.0",
    "typescript": "5.3.2",
    "vite": "^4.5.1",
    "vitest": "^0.34.6"
  }
}
